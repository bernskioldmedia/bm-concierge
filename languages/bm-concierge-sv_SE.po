#: assets/scripts/dist/block-editor.js:1
#: assets/scripts/src/components/panels/accessibility-review.js:35
#: assets/scripts/src/components/panels/layout.js:34
#: assets/scripts/src/components/panels/onpage.js:36
#: assets/scripts/src/components/panels/proofreading.js:35
msgid "72 hours (24h express)"
msgstr "72 timmar (24h express)"

#: assets/scripts/dist/block-editor.js:1
#: assets/scripts/src/components/panels/accessibility-review.js:36
#: assets/scripts/src/components/panels/layout.js:35
